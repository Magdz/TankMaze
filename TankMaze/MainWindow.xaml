<Window x:Class="TankMaze.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TankMaze"
        mc:Ignorable="d"
        WindowState="Maximized"
        Title="TankMaze" Height="{DynamicResource {x:Static SystemParameters.MaximizedPrimaryScreenHeightKey}}" Width="{DynamicResource {x:Static SystemParameters.MaximizedPrimaryScreenWidthKey}}" ResizeMode="NoResize" Icon="Assets/WindowIcon.png">
    <Grid VerticalAlignment="Top">
        <Frame x:Name="_NavigationFrame" Height="993" Width="2040" NavigationUIVisibility="Hidden" Grid.ColumnSpan="2"/>
    </Grid>
</Window>